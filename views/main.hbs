<div class="container">
    <div class="row">
        <div class="col">
            <h1>{{name}}</h1>
            <h2>SIUUUU</h2>
        </div>
    </div>
    <form method="post" action="/" class="form-inline mb-3" id="mem" name="key">
        <div class="form-group mr-5">
            <label for="code" class="mr-3">ApiKey</label>
            <input type="text" class="form-control" id="code" name="code">
        </div>
        <input class="btn btn-primary" type="submit" name="status" value="Відправити">
    </form>
    <form method="post" action="/createFields" class="form-inline mb-3" id="nen" name="fields">
        <input class="btn btn-primary" type="submit" name="status" value="Створити поля">
    </form>
    <form method="post" action="/createWebhook" class="form-inline mb-3" id="web" name="webhook">
        <input class="btn btn-primary" type="submit" name="status" value="Створити Webhook">
    </form>
    <h4 id="result">Статус підключення: {{status}}</h4>
    <h4 id="fields">Поля: {{createFields}}</h4>
    <h4 id="webhook">Webhook: {{webhook}}</h4>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.5/dist/umd/popper.min.js" integrity="sha384-Xe+8cL9oJa6tN/veChSP7q+mnSPaj5Bcu9mPX5F5xIGE0DVittaqT5lorf0EI7Vk" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.min.js" integrity="sha384-kjU+l4N0Yf4ZOJErLsIcvOU2qSb74wXpOhqTvwVx3OElZRweTnQ6d31fXEoRD1Jy" crossorigin="anonymous"></script>
    <script src="index.js"></script>
</div>